{
	"compilerOptions": {
		"target": "es5",
		"lib": [ "es6", "dom" ],
		"module": "es6",
		"moduleResolution": "node",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"preserveConstEnums": true,
		"declaration": true,
		"sourceMap": true,
		"baseUrl": ".",
		"outDir": "dist"
	},
	"exclude": [
		"node_modules",
		"dist"
	],
	"compileOnSave": false,
	"buildOnSave": false,
	"atom": {
		"rewriteTsconfig": false
	}
}

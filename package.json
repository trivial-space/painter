{
  "name": "tvs-renderer",
  "version": "0.1.13",
  "description": "trivialspace renderer - a webgl rendering engine",
  "main": "dist/tvs-renderer.js",
  "typings": "lib/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trivial-space/renderer.git"
  },
  "keywords": [
    "webgl",
    "graphics engine"
  ],
  "author": "Thomas Gorny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trivial-space/renderer/issues"
  },
  "homepage": "https://github.com/trivial-space/renderer#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "chokidar-socket-emitter": "^0.5.2",
    "gulp": "^3.9.1",
    "gulp-connect": "^4.0.0",
    "gulp-livereload": "^3.8.1",
    "jspm": "^0.17.0-beta.29",
    "ts-loader": "^0.8.2",
    "typescript": "^2.1.1",
    "webpack": "^1.13.1",
    "webpack-stream": "^3.2.0"
  },
  "jspm": {
    "name": "tvs-renderer",
    "main": "dist/tvs-renderer.js",
    "dependencies": {
      "primitive-cube": "npm:primitive-cube@^2.0.0",
      "source-map-support": "npm:source-map-support@^0.4.6"
    },
    "devDependencies": {
      "gl-matrix": "npm:gl-matrix@^2.3.2",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "text": "github:systemjs/plugin-text@^0.0.8",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.12",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^5.0.8",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.6.0"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0"
    },
    "overrides": {
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:typescript@2.1.1": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "dependencies": {}
}
